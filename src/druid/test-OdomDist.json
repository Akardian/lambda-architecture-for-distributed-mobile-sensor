{
  "type": "kafka",
  "spec": {
    "ioConfig": {
      "type": "kafka",
      "topic": "find3Streaming-test-odomDist-output",
      "consumerProperties": {
        "bootstrap.servers": "kafka01:9092"
      },
      "inputFormat": {
        "type": "json"
      },
      "useEarliestOffset": true
    },
    "tuningConfig": {
      "type": "kafka"
    },
    "dataSchema": {
      "dataSource": "find3Streaming-test-odomDist",
      "granularitySpec": {
        "type": "uniform",
        "queryGranularity": "NONE",
        "segmentGranularity": "HOUR",
        "rollup": false
      },
      "timestampSpec": {
        "column": "max(timestampKafkaIn)",
        "format": "iso"
      },
      "dimensionsSpec": {
        "dimensions": [
          {
            "type": "double",
            "name": "aggdistancelocal$(secs, nsecs, x, y, z)"
          },
          "senderName"
        ]
      }
    }
  }
}
